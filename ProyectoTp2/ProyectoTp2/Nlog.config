<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<targets>
		<target name="logfile" xsi:type="File" fileName="file.txt" />
		<target name="logconsole" xsi:type="File" fileName="fileinfo.txt"/>
		<target name="logfilenameWarner" xsi:type="File" fileName="filewarn.txt"/>
	</targets>

	<rules>
		<logger name="*" minlevel="Info" maxlevel="Info" writeTo="logconsole" />
		<logger name="*" minlevel="Trace" maxlevel="Debug" writeTo="logfile" />
		<logger name="*" minlevel="Warn" maxlevel="Fatal" writeTo="logfilenameWarner" />
	</rules>
</nlog>
