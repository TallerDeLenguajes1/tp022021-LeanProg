<h1> CARGAR EMPLEADOS </h1>
<form asp-action="VerEmpleado" asp-controller="Home" method="post">
    <div class="form-group">
        <label for="formGroupExampleInput">Nombre</label>
        <input type="text" name="nombre" class="form-control" id="Nomb">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput">Apellido</label>
        <input type="text" name="apellido" class="form-control" id="Ape">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput2">Direccion</label>
        <input type="text" name="dire" class="form-control" id="dire">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput2">Fecha de Nacimeinto</label>
        <input type="text" name="Fna" class="form-control" id="Fnac">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput2">Fecha de ingreso</label>
        <input type="text" name="Fing" class="form-control" id="Fing">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput2">Titulo</label>
        <input type="text" name="titulo" class="form-control" id="tit">
    </div>
    <div class="form-group">
        <label for="formGroupExampleInput2">Matricula</label>
        <input type="number" name="matri" class="form-control" id="Mat">
    </div>
    <input type="submit" name="btn" value="Cargar" />
</form>
